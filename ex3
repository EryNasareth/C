#include <stdio.h>

int sec(int H, int M, int S) {
	int total = (H * 3600) + (M * 60) + S;
	return total;
}

int main() {
	int H, M, S;
	printf("digite as horas ");
	scanf("%d", &H);
	printf("digite os minutos ");
	scanf("%d", &M);
	printf("digite os segundos ");
	scanf("%d", &S);
	int result = sec(H, M, S);
	printf("%d", result);
	return 0;
}
